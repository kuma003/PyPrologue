{
  "processing": {
    "multi_thread": true,
    "multi_thread_count": 4
  },

  "simulation": {
    "dt": 0.0001,
    "detect_peak_threshold": 15.0,

    "scatter": {
      "wind_speed_min": 1.0,
      "wind_speed_max": 7.0,
      "wind_dir_interval": 22.5
    }
  },

  "result": {
    "precision": 8,
    "step_save_interval": 10
  },

  "wind_model": {
    "power_constant": 6.0,
    "power_low_base_alt": 5.0,
    "type": "only_powerlaw",
    "realdata_filename": "sample.csv"
  },

  "atmosphere": {
    "base_pressure_pascal": 101325,
    "base_temperature_celsius": 15
  }
}
